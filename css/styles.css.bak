@font-face {
  font-family: qingxin;
  src: url(../font/QingXin.ttf);
}
@font-face {
  font-family: droid-sans;
  src: url(../font/DroidSans.ttf);
}
body{
  font-family: qingxin;
}
hr{
  border-top: 1px dashed;
}
p{
  margin: 2%;
}
li{
  margin: 2%;
}
h1{
  margin-top: 10%;
}
#container{
  margin: 5%;
}
#name_header
{
  margin-top: 10%;
  margin-bottom: 10%;
  text-align: center;
}
#container_header_img{
  margin-top: 15%;
  display: flex;
  justify-content: center;
  margin-bottom: 15%;
}
#img_header{
  height: 200px;
  width: 200px;
  border-radius: 100px;
}
.progress{
  margin: 15%;
  display: flex;
  justify-content: center;
}
.container_card{
  display: flex;
  flex-wrap: wrap;
}
.card_content{
  min-width: 200px;
  width: 432px;
  height: 124px;
  margin: 16px;
}
.img_card{
  max-width: 75%;
  height: 124px;
}